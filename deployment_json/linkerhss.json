{
  "name": "linkerhss",
  "description": "linkerhss",
  "group": {
    "id": "linkerhss",
    "apps": [{
      "id": "stack",
      "dependencies": [
        "decoder"
      ],
      "labels": {
        "HAPROXY_GROUP": "linkermgmt"
      },
      "container": {
        "type": "DOCKER",
        "docker": {
          "image": "linkerrepository/linker_stack",
          "privileged": true,
          "parameters": [{
            "key": "net-alias",
            "value": "stack.linker.com"
          }, {
            "key": "net",
            "value": "myoverlay"
          }],
          "network": "BRIDGE",
          "forcePullImage": true,
          "portMappings": [{
            "containerPort": 3869,
            "hostPort": 0,
            "servicePort": 10069,
            "protocol": "tcp"
          }, {
            "containerPort": 3868,
            "hostPort": 0,
            "servicePort": 10068,
            "protocol": "tcp"
          }, {
            "containerPort": 8000,
            "hostPort": 0,
            "servicePort": 10089,
            "protocol": "udp"
          }]
        }
      },
      "instances": 1,
      "cpus": 0.1,
      "mem": 4096,
      "env": {
        "TRANSMIT_PACKAGE_NUMBER_LOW_THRESHOLD": "20",
        "TRANSMIT_PACKAGE_NUMBER_HIGH_THRESHOLD": "100",
        "IDEAL_NODE_NUMBER": "2",
        "ALERT_ENABLE": "true"
      }
    }, {
      "id": "decoder",
      "dependencies": [
        "router"
      ],
      "container": {
        "type": "DOCKER",
        "docker": {
          "image": "linkerrepository/linker_decoder",
          "privileged": true,
          "parameters": [{
            "key": "net-alias",
            "value": "decoder.linker.com"
          }, {
            "key": "net",
            "value": "myoverlay"
          }],
          "forcePullImage": true,
          "network": "BRIDGE"
        }
      },
      "instances": 2,
      "cpus": 0.1,
      "mem": 2048
    }, {
      "id": "router",
      "dependencies": [
        "hss"
      ],
      "labels": {
        "HAPROXY_GROUP": "linkermgmt"
      },
      "container": {
        "type": "DOCKER",
        "docker": {
          "image": "linkerrepository/linker_hss_router",
          "privileged": true,
          "parameters": [{
            "key": "net-alias",
            "value": "router.linker.com"
          }, {
            "key": "net",
            "value": "myoverlay"
          }],
          "portMappings": [{
            "containerPort": 8000,
            "hostPort": 0,
            "protocol": "udp",
            "servicePort": 10088
          }],
          "forcePullImage": true,
          "network": "BRIDGE"
        }
      },
      "instances": 1,
      "cpus": 0.1,
      "mem": 4096,
      "env": {
        "TRANSMIT_PACKAGE_NUMBER_LOW_THRESHOLD": "20",
        "TRANSMIT_PACKAGE_NUMBER_HIGH_THRESHOLD": "100",
        "IDEAL_NODE_NUMBER": "2",
        "ALERT_ENABLE": "true"
      }
    }, {
      "id": "hss",
      "dependencies": [
        "es"
      ],
      "labels": {
        "HAPROXY_GROUP": "linkermgmt"
      },
      "container": {
        "type": "DOCKER",
        "docker": {
          "image": "linkerrepository/linker_hss",
          "privileged": true,
          "parameters": [{
            "key": "net-alias",
            "value": "hss.linker.com"
          }, {
            "key": "net",
            "value": "myoverlay"
          }],
          "portMappings": [{
            "containerPort": 5001,
            "hostPort": 0,
            "protocol": "tcp",
            "servicePort": 10051
          }, {
            "containerPort": 5002,
            "hostPort": 0,
            "protocol": "tcp",
            "servicePort": 10052
          }],
          "forcePullImage": true,
          "network": "BRIDGE"
        }
      },
      "instances": 2,
      "cpus": 0.1,
      "mem": 2048
    }, {
      "id": "es",
      "labels": {
        "HAPROXY_GROUP": "linkermgmt"
      },
      "container": {
        "type": "DOCKER",
        "docker": {
          "image": "linkerrepository/linker_es",
          "parameters": [{
            "key": "net-alias",
            "value": "es.hss.com"
          }, {
            "key": "net-alias",
            "value": "esmaster.linker.com"
          }, {
            "key": "net",
            "value": "myoverlay"
          }],
          "privileged": true,
          "portMappings": [{
            "containerPort": 9200,
            "hostPort": 0,
            "protocol": "tcp",
            "servicePort": 10092
          }, {
            "containerPort": 9300,
            "hostPort": 0,
            "protocol": "tcp",
            "servicePort": 10093
          }],
          "forcePullImage": true,
          "network": "BRIDGE"
        }
      },
      "instances": 2,
      "cpus": 0.1,
      "mem": 2048,
      "env": {
        "CPU_USAGE_LOW_THRESHOLD": "20",
        "CPU_USAGE_HIGH_THRESHOLD": "50",
        "IDEAL_NODE_NUMBER": "2",
        "ALERT_ENABLE": "true"
      }
    }]
  }
}
